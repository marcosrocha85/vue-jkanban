{
  "name": "vue-jkanban",
  "version": "1.0.0",
  "description": "Vue component from riktar/jkanban",
  "main": "vue-jkanban.js",
  "watch": {
    "build": "vue-jkanban.js"
  },
  "scripts": {
    "watch_build": "npm-watch build",
    "build": "npm run scripts && npm run styles",
    "scripts": "browserify ./vue-jkanban.js -o dist/vue-jkanban.js && uglifyjs -m -c -o dist/vue-jkanban.min.js dist/vue-jkanban.js",
    "styles": "npm-css ./vue-jkanban.css > dist/vue-jkanban.css && cleancss dist/vue-jkanban.css -o dist/vue-jkanban.min.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcosrocha85/vue-jkanban.git"
  },
  "homepage": "https://marcosrocha.net/vue-jkanban",
  "keywords": [
    "jkanban",
    "vue",
    "kanban",
    "js",
    "drag",
    "todo list",
    "javascript",
    "plugin"
  ],
  "author": "Marcos Rocha",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/marcosrocha85/vue-jkanban/issues"
  },
  "dependencies": {
    "jkanban": "^1.0.1",
    "npm": "^6.6.0",
    "npm-watch": "^0.6.0"
  },
  "devDependencies": {
    "browserify": "11.0.0",
    "clean-css": "^3.3.6",
    "npm-css": "^0.2.3",
    "uglify-js": "^3.3.4"
  }
}
